{
  "expo": {
    "name": "zombiemanager",
    "slug": "zombiemanager",
    "description": "Zombie manager for adding and relocating zombies",
    "privacy": "hidden",
    "githubUrl": "https://github.com/LyonRincon/ZombieManager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#131B24"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#131B24"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
